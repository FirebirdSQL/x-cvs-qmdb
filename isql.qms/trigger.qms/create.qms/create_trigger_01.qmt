<test>
 <class>firebird.FirebirdISQLTest</class>
 <argument name='target_group'>
  <text>.*</text>
 </argument>
 <argument name='description'>
  <text>CREATE TRIGGER

Dependencies:
CREATE DATABASE
CREATE TABLE</text>
 </argument>
