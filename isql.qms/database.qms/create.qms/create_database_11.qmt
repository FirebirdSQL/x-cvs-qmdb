  FILE &quot;$(DATABASE_LOCATION)TEST.G01&quot; STARTING AT PAGE 601
  FILE &quot;$(DATABASE_LOCATION)TEST.G02&quot; STARTING AT PAGE 1001;

SELECT cast(RDB$FILE_NAME as varchar(60)),RDB$FILE_SEQUENCE,RDB$FILE_START,RDB$FILE_LENGTH 
  FROM RDB$FILES;

DROP DATABASE;
</text></argument><argument name="author"><text>skopalik</text></argument><argument name="stdout"><text>CAST                                                         RDB$FILE_SEQUENCE RDB$FILE_START RDB$FILE_LENGTH
